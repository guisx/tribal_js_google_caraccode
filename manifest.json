{
	"manifest_version": 2,
	"name": "Accents et caractères spéciaux",
	"description": "accents et caractères spéciaux en HTML", 
	"version": "1.0", 
	"permissions": [ 
		"clipboardWrite", 
		"clipboardRead",
		"http://*/*",
  		"https://*/*"
	],	
	"browser_action": {  
		"default_icon" : "icon.png",
		"default_popup": "popup.html"
	},
	"icons": { 
		"128" : "icon_128.png" 
	}

}